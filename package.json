{
  "name": "@rafaeelricco/commit-tools",
  "version": "0.1.5",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "commit": "./dist/index.js"
  },
  "scripts": {
    "start": "bun index.ts",
    "setup": "bun run start setup",
    "doctor": "bun run start doctor",
    "test": "bun test",
    "compile": "bun run build",
    "build": "bun build ./index.ts --outdir ./dist --target bun --minify --sourcemap",
    "install:global": "bun link",
    "format": "npx prettier . --write",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rafaeelricco/commit-tools"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.6",
    "@types/ink-text-input": "^2.0.5",
    "@types/luxon": "^3.7.1",
    "@types/react": "^19.2.14",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "react-devtools-core": "^7.0.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@google/generative-ai": "^0.24.1",
    "cli-table3": "^0.6.5",
    "fluture": "^14.0.0",
    "google-auth-library": "^10.5.0",
    "ink": "^6.8.0",
    "luxon": "^3.7.2",
    "open": "^11.0.0",
    "picocolors": "^1.1.1",
    "react": "^19.2.4"
  }
}
