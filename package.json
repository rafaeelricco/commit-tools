  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.6",
    "@types/luxon": "^3.7.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "@google/generative-ai": "^0.24.1",
    "cli-table3": "^0.6.5",
    "fluture": "^14.0.0",
    "luxon": "^3.7.2",
    "picocolors": "^1.1.1"
  }
}
