{
  "name": "commit-tools",
  "module": "index.ts",
  "type": "module",
  "bin": {
    "commit": "./dist/index.js"
  },
  "scripts": {
    "start": "bun index.ts",
    "setup": "bun run start setup",
    "doctor": "bun run start doctor",
    "test": "bun test",
